    "type": "library",
    "require": {
        "guzzlehttp/guzzle": ">=6.0",
        "illuminate/support": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hitaaksata\\BpjsServices\\": "src/"
        }
